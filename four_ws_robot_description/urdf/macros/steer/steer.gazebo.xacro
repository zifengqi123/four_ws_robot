<?xml version="1.0"?>
<robot xmlns:xacro="http://ros.org/wiki/xacro">
    <xacro:macro name="steer_gazebo" params="prefix">
        <gazebo reference="${prefix}_steer_link">
            <selfCollide>false</selfCollide>
            <mu1 value="0.05" />
            <mu2 value="0.05" />
        </gazebo>
    </xacro:macro>
</robot>